    "buildDev": "cd ./pz-builder && npm run buildDev",
    "buildProd": "cd ./pz-builder && npm run buildProd",
    "watch": "cd ./pz-builder && npm run watch",
    "testsIn": "mocha --recursive",
    "serverSmokeTests": "npm run testsIn pz-server/build/test/smoke",
    "serverIntegTests": "npm run testsIn pz-server/build/test/integration",
