{
  "name": "Praisee",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cd ./pz-server && export DEBUG=* && npm start",
    "startWin": "cd ./pz-server && SET DEBUG=* && npm start",
    "startQuiet": "cd ./pz-server && npm start",
    "typingsInstall": "cd ./pz-client && typings install && cd ../pz-server && typings install && cd ../pz-support && typings install",
    "postinstall": "npm run typingsInstall",
    "buildDev": "cd ./pz-builder && npm run buildDev",
    "watch": "cd ./pz-builder && npm run watch",
    "tests": "mocha --es_staging --harmony_tostring --harmony_array_includes --harmony_rest_parameters --harmony_modules --harmony_regexps --harmony_proxies --harmony_unicode_regexps --harmony_reflect --harmony_destructuring --harmony_new_target --recursive",
    "serverSmokeTests": "mocha --require source-map-support/register --es_staging --harmony_tostring --harmony_array_includes --harmony_rest_parameters --harmony_modules --harmony_regexps --harmony_proxies --harmony_unicode_regexps --harmony_reflect --harmony_destructuring --harmony_new_target --recursive pz-server/build/test/smoke",
    "serverIntegrationTests": "mocha --require source-map-support/register --es_staging --harmony_tostring --harmony_array_includes --harmony_rest_parameters --harmony_modules --harmony_regexps --harmony_proxies --harmony_unicode_regexps --harmony_reflect --harmony_destructuring --harmony_new_target --recursive pz-server/build/test/integration",
    "serverIntegTests": "npm run serverIntegrationTests",
    "serverUnitTests": "mocha --require source-map-support/register --es_staging --harmony_tostring --harmony_array_includes --harmony_rest_parameters --harmony_modules --harmony_regexps --harmony_proxies --harmony_unicode_regexps --harmony_reflect --harmony_destructuring --harmony_new_target --recursive pz-server/build/test/unit"
  },
